{
  "commands": {
    "build": [
      "cd react_template",
      "npm install",
      "npm run build"
    ]
  },
  "options": {
    "node": "20"
  },
  "outputs": {
    "dir": "react_template/dist",
    "include": [
      "**/*"
    ]
  },
  "root": "react_template",
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000",
    "VITE_APP_TITLE": "来贺白酒官网"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "cache-control": "public, max-age=0, must-revalidate"
      }
    }
  ]
} 