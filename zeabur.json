{
  "root": "react_template",
  "build": {
    "command": "cd react_template && npm install && npm run build",
    "output": "react_template/dist"
  },
  "install": {
    "command": "cd react_template && npm install"
  },
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 