{
  "buildConfig": {
    "type": "vite",
    "command": "npm run build",
    "output": "react_template/dist"
  },
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 