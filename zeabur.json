{
  "root": "react_template",
  "buildConfig": {
    "type": "vite",
    "command": "npm run build",
    "output": "dist"
  },
  "commands": {
    "build": "cd react_template && npm install && npm run build",
    "start": "cd react_template && npm run preview"
  },
  "env": {
    "NODE_ENV": "production",
    "PORT": "3000"
  },
  "framework": "vite",
  "language": "javascript",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 