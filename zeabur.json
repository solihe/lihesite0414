{
  "root": "react_template",
  "build": {
    "command": "npm run build",
    "output": "dist"
  },
  "install": {
    "command": "npm install"
  },
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 